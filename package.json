{
  "name": "quiz-cms",
  "private": true,
  "version": "0.1.0",
  "description": "AstroQuiz Strapi CMS with comprehensive API testing suite",
  "scripts": {
    "build": "strapi build",
    "develop": "strapi develop",
    "start": "strapi start",
    "strapi": "strapi",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:api": "jest scripts/api.test.js",
    "test:performance": "jest scripts/performance.test.js",
    "test:quiz": "jest scripts/quiz-engine.test.js",
    "test:all": "jest scripts/api.test.js scripts/performance.test.js scripts/quiz-engine.test.js",
    "test:services": "jest --testPathPattern=src/services",
    "test:routes": "jest --testPathPattern=src/routes",
    "test:middlewares": "jest --testPathPattern=src/middlewares",
    "test:unit": "jest --testPathPattern=src/",
    "railway:build": "rm -rf dist build .cache node_modules/.cache && npm run build && echo \"BUILD COMPLETE - TIMESTAMP: $(date)\"",
    "railway:start": "NODE_ENV=production npm run start",
    "postbuild": "echo 'Build completed successfully'",
    "health": "curl -f http://localhost:1337/api/health || exit 1",
    "generate-keys": "node scripts/generate-keys.js",
    "setup-env": "node scripts/generate-keys.js",
    "deploy:verify": "node scripts/verify-railway-deploy.js",
    "deploy:staging-keys": "node scripts/generate-railway-env.js staging",
    "deploy:production-keys": "node scripts/generate-railway-env.js production",
    "performance:report": "node scripts/generate-performance-report.js",
    "performance:test": "node scripts/load-test.js",
    "performance:light": "node scripts/load-test.js https://astroquiz-production.up.railway.app light",
    "performance:heavy": "node scripts/load-test.js https://astroquiz-production.up.railway.app heavy stress",
    "analytics:export": "curl -s https://astroquiz-production.up.railway.app/api/dashboard/metrics/export",
    "logs:export": "node -e \"strapi.service('global::logger').exportLogs()\"",
    "db:analyze": "node -e \"strapi.service('global::query-optimizer').analyzeQueryPerformance().then(console.log)\"",
    "assets:organize": "node scripts/organize-assets.js",
    "assets:optimize": "node scripts/optimize-assets.js",
    "assets:all": "npm run assets:organize && npm run assets:optimize"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "jest": "^29.7.0",
    "sqlite3": "^5.1.7",
    "supertest": "^6.3.3",
    "ts-jest": "^29.4.6"
  },
  "dependencies": {
    "@strapi/plugin-cloud": "5.23.0",
    "@strapi/plugin-users-permissions": "^5.23.5",
    "@strapi/strapi": "^5.23.5",
    "axios": "^1.6.0",
    "better-sqlite3": "11.3.0",
    "csv-parse": "^6.1.0",
    "csv-stringify": "^6.6.0",
    "dotenv": "^17.2.1",
    "firebase-admin": "^13.6.0",
    "pg": "^8.11.0",
    "pg-connection-string": "^2.6.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.30.1",
    "styled-components": "^6.1.19"
  },
  "author": {
    "name": "AstroQuiz Team"
  },
  "strapi": {
    "uuid": "your-project-uuid"
  },
  "engines": {
    "node": ">=18.0.0 <=20.x.x",
    "npm": ">=6.0.0"
  },
  "license": "MIT"
}
